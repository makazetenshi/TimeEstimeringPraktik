<Window x:Class="praktik_estimering.exam"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Time Estimation - Exam Activities" Height="350" Width="400" Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="202*"/>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="125*"/>
        </Grid.ColumnDefinitions>

        <DataGrid x:Name="datagridExam" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="300" Width="292" Grid.ColumnSpan="3"/>
        <Button Content="Continue" HorizontalAlignment="Left" Margin="38.2,261,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Grid.Column="2"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="38.2,288,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Grid.Column="2"/>

    </Grid>
</Window>
