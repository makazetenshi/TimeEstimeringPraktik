<Window x:Class="praktik_estimering.DayAktivities"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DayAktivities" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="listboxActivities" HorizontalAlignment="Left" Height="250" Margin="10,10,0,0" VerticalAlignment="Top" Width="157"/>
        <Label Content="Choose activity" HorizontalAlignment="Left" Margin="172,10,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ComboboxActivities" HorizontalAlignment="Left" Margin="172,41,0,0" VerticalAlignment="Top" Width="110"/>
        <Label Content="number of days" HorizontalAlignment="Left" Margin="172,68,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textboxDays" HorizontalAlignment="Left" Height="23" Margin="172,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="110"/>
        <Button x:Name="buttonDeleteFromList" Content="Delete" HorizontalAlignment="Left" Margin="187,184,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="buttonNext" Content="Next" HorizontalAlignment="Left" Margin="207,238,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="buttonAdd" Content="Add" HorizontalAlignment="Left" Margin="187,139,0,0" VerticalAlignment="Top" Width="75" Click="ButtonAddClicked"/>

    </Grid>
</Window>
